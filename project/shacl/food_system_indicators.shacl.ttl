@prefix dct: <http://purl.org/dc/terms/> .
@prefix fsi: <https://w3id.org/linkml/examples/fsi> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

fsi:Datapoint a sh:NodeShape ;
    sh:closed true ;
    sh:description "Food system indicator datapoint." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype <xsd:double> ;
            sh:description "connects a quantity value to a number" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path fsi:has_numeric_value ],
        [ sh:datatype <UO:0000000> ;
            sh:description "connects a quantity value to a unit" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path fsi:has_unit ],
        [ sh:datatype <xsd:string> ;
            sh:description "A human-readable description of an entity." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path dct:description ],
        [ sh:datatype <xsd:string> ;
            sh:description "An IRI for an entity. This is determined by the id using expansion rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path fsi:iri ],
        [ sh:datatype <xsd:string> ;
            sh:description "A human-readable name for an attribute or entity." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path rdfs:label ],
        [ sh:datatype <xsd:string> ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path fsi:datapoint_of ],
        [ sh:datatype <xsd:string> ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path fsi:id ] ;
    sh:targetClass fsi:Datapoint .

fsi:Entity a sh:NodeShape ;
    sh:closed false ;
    sh:description "Root class for all things and informational relationships, real or imagined." ;
    sh:ignoredProperties ( rdf:type fsi:datapoint_of fsi:spatial_scope fsi:has_unit fsi:has_numeric_value ) ;
    sh:property [ sh:datatype <xsd:string> ;
            sh:description "A unique identifier for an entity." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path fsi:id ],
        [ sh:datatype <xsd:string> ;
            sh:description "A human-readable name for an attribute or entity." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path rdfs:label ],
        [ sh:datatype <xsd:string> ;
            sh:description "A human-readable description of an entity." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path dct:description ],
        [ sh:datatype <xsd:string> ;
            sh:description "An IRI for an entity. This is determined by the id using expansion rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path fsi:iri ] ;
    sh:targetClass fsi:Entity .

fsi:Indicator a sh:NodeShape ;
    sh:closed true ;
    sh:description "Food system indicator." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype <xsd:string> ;
            sh:description "unique identifier of the indicator" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path schema1:id ],
        [ sh:datatype <xsd:string> ;
            sh:description "denomination of the indicator" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path schema1:name ],
        [ sh:description "reference to the spatial unit that the indicator describes" ;
            sh:in ( "Eu" "EuMemberStates" "Regional" "Local" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path fsi:spatial_scope ],
        [ sh:datatype <xsd:string> ;
            sh:description "An IRI for an entity. This is determined by the id using expansion rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path fsi:iri ],
        [ sh:datatype <xsd:string> ;
            sh:description "concise text that provides the meaning of the identifier" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path schema1:description ] ;
    sh:targetClass fsi:Indicator .

fsi:NamedThing a sh:NodeShape ;
    sh:closed true ;
    sh:description "A databased entity or concept/class." ;
    sh:ignoredProperties ( rdf:type fsi:datapoint_of fsi:spatial_scope fsi:has_unit fsi:has_numeric_value ) ;
    sh:property [ sh:datatype <xsd:string> ;
            sh:description "An IRI for an entity. This is determined by the id using expansion rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path fsi:iri ],
        [ sh:datatype <xsd:string> ;
            sh:description "A human-readable name for an attribute or entity." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path rdfs:label ],
        [ sh:datatype <xsd:string> ;
            sh:description "A human-readable description of an entity." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path dct:description ],
        [ sh:datatype <xsd:string> ;
            sh:description "A unique identifier for an entity." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path fsi:id ] ;
    sh:targetClass fsi:NamedThing .

fsi:QuantityValue a sh:NodeShape ;
    sh:closed true ;
    sh:description "A value of an attribute that is quantitative and measurable, expressed as a combination of a unit and a numeric value" ;
    sh:ignoredProperties ( rdf:type fsi:datapoint_of ) ;
    sh:property [ sh:datatype <xsd:double> ;
            sh:description "connects a quantity value to a number" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path fsi:has_numeric_value ],
        [ sh:datatype <xsd:string> ;
            sh:description "An IRI for an entity. This is determined by the id using expansion rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path fsi:iri ],
        [ sh:datatype <xsd:string> ;
            sh:description "A human-readable name for an attribute or entity." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path rdfs:label ],
        [ sh:datatype <UO:0000000> ;
            sh:description "connects a quantity value to a unit" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path fsi:has_unit ],
        [ sh:datatype <xsd:string> ;
            sh:description "A unique identifier for an entity." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path fsi:id ],
        [ sh:datatype <xsd:string> ;
            sh:description "A human-readable description of an entity." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path dct:description ] ;
    sh:targetClass fsi:QuantityValue .

