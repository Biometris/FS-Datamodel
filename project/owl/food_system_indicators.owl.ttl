@prefix IAO: <http://purl.obolibrary.org/obo/IAO_> .
@prefix UO: <http://purl.obolibrary.org/obo/UO_> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix fsi: <https://w3id.org/linkml/examples/fsi> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

fsi:.owl.ttl a owl:Ontology ;
    rdfs:label "food_system_indicators" ;
    dct:title "food systems indicators schema" ;
    skos:definition "Model for the indicators on the sustainability performance of food Systems within Europe." .

fsi:IndicatorDatapointCollection a owl:Class ;
    rdfs:label "IndicatorDatapointCollection" ;
    dct:title "indicator datapoint collection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom fsi:IndicatorDatapoint ;
            owl:onProperty fsi:indicator_datapoints ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty fsi:indicator_datapoints ] ;
    skos:definition "Collection of food system indicator datapoints." ;
    skos:inScheme fsi: .

fsi:IndicatorDatapoint a owl:Class ;
    rdfs:label "IndicatorDatapoint" ;
    dct:title "indicator datapoint" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:measurement_of ],
        [ a owl:Restriction ;
            owl:allValuesFrom fsi:Indicator ;
            owl:onProperty fsi:measurement_of ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty fsi:measurement_of ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty fsi:id ],
        fsi:QuantityValue ;
    skos:definition "Food system indicator datapoint." ;
    skos:inScheme fsi: .

fsi:node_property a owl:DatatypeProperty ;
    rdfs:label "node property" ;
    rdfs:domain fsi:Entity ;
    skos:definition "A grouping for any property that holds between a node and a value." ;
    skos:inScheme fsi: .

UO:0000000 a rdfs:Datatype ;
    owl:equivalentClass xsd:string .

<https://w3id.org/linkml/examples/fsiEconomicThematicArea#DevelopmentAndLogistics> a owl:Class ;
    rdfs:label "DevelopmentAndLogistics" ;
    rdfs:subClassOf fsi:EconomicThematicArea .

<https://w3id.org/linkml/examples/fsiEconomicThematicArea#FairEconomicViabilityInFoodValueChain> a owl:Class ;
    rdfs:label "FairEconomicViabilityInFoodValueChain" ;
    rdfs:subClassOf fsi:EconomicThematicArea .

<https://w3id.org/linkml/examples/fsiEnvironmentalThematicArea#Biodiversity> a owl:Class ;
    rdfs:label "Biodiversity" ;
    rdfs:subClassOf fsi:EnvironmentalThematicArea .

<https://w3id.org/linkml/examples/fsiEnvironmentalThematicArea#ClimateChange> a owl:Class ;
    rdfs:label "ClimateChange" ;
    rdfs:subClassOf fsi:EnvironmentalThematicArea .

<https://w3id.org/linkml/examples/fsiEnvironmentalThematicArea#CrossCuttingAreas> a owl:Class ;
    rdfs:label "CrossCuttingAreas" ;
    rdfs:subClassOf fsi:EnvironmentalThematicArea .

<https://w3id.org/linkml/examples/fsiEnvironmentalThematicArea#PollutionAntimicrobials> a owl:Class ;
    rdfs:label "PollutionAntimicrobials" ;
    rdfs:subClassOf fsi:EnvironmentalThematicArea .

<https://w3id.org/linkml/examples/fsiEnvironmentalThematicArea#SustainableUseOfResources> a owl:Class ;
    rdfs:label "SustainableUseOfResources" ;
    rdfs:subClassOf fsi:EnvironmentalThematicArea .

<https://w3id.org/linkml/examples/fsiHorizontalThematicArea#Governance> a owl:Class ;
    rdfs:label "Governance" ;
    rdfs:subClassOf fsi:HorizontalThematicArea .

<https://w3id.org/linkml/examples/fsiHorizontalThematicArea#Resilience> a owl:Class ;
    rdfs:label "Resilience" ;
    rdfs:subClassOf fsi:HorizontalThematicArea .

<https://w3id.org/linkml/examples/fsiSocialThematicArea#FairInclusiveAndEthicalFoodSystem> a owl:Class ;
    rdfs:label "FairInclusiveAndEthicalFoodSystem" ;
    rdfs:subClassOf fsi:SocialThematicArea .

<https://w3id.org/linkml/examples/fsiSocialThematicArea#FoodEnvironment> a owl:Class ;
    rdfs:label "FoodEnvironment" ;
    rdfs:subClassOf fsi:SocialThematicArea .

<https://w3id.org/linkml/examples/fsiSocialThematicArea#NutritionAndHealth> a owl:Class ;
    rdfs:label "NutritionAndHealth" ;
    rdfs:subClassOf fsi:SocialThematicArea .

<https://w3id.org/linkml/examples/fsiSpatialScopeType#Eu> a owl:Class ;
    rdfs:label "Eu" ;
    rdfs:subClassOf fsi:SpatialScopeType .

<https://w3id.org/linkml/examples/fsiSpatialScopeType#EuMemberStates> a owl:Class ;
    rdfs:label "EuMemberStates" ;
    rdfs:subClassOf fsi:SpatialScopeType .

<https://w3id.org/linkml/examples/fsiSpatialScopeType#Local> a owl:Class ;
    rdfs:label "Local" ;
    rdfs:subClassOf fsi:SpatialScopeType .

<https://w3id.org/linkml/examples/fsiSpatialScopeType#Regional> a owl:Class ;
    rdfs:label "Regional" ;
    rdfs:subClassOf fsi:SpatialScopeType .

<https://w3id.org/linkml/examples/fsiSustainabilityDimension#Economic> a owl:Class ;
    rdfs:label "Economic" ;
    rdfs:subClassOf fsi:SustainabilityDimension .

<https://w3id.org/linkml/examples/fsiSustainabilityDimension#Environmental> a owl:Class ;
    rdfs:label "Environmental" ;
    rdfs:subClassOf fsi:SustainabilityDimension .

<https://w3id.org/linkml/examples/fsiSustainabilityDimension#Horizontal> a owl:Class ;
    rdfs:label "Horizontal" ;
    rdfs:subClassOf fsi:SustainabilityDimension .

<https://w3id.org/linkml/examples/fsiSustainabilityDimension#Social> a owl:Class ;
    rdfs:label "Social" ;
    rdfs:subClassOf fsi:SustainabilityDimension .

fsi:ThematicArea a owl:Class ;
    rdfs:label "ThematicArea" ;
    dct:title "thematic area" ;
    skos:definition "a thematic area" ;
    skos:exactMatch linkml:Any ;
    skos:inScheme fsi: .

fsi:indicator_datapoints a owl:ObjectProperty ;
    rdfs:label "indicator datapoints" ;
    dct:title "indicator datapoints" ;
    skos:inScheme fsi: .

fsi:QuantityValue a owl:Class ;
    rdfs:label "QuantityValue" ;
    dct:title "quantity value" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty fsi:has_unit ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty fsi:has_numeric_value ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:has_numeric_value ],
        [ a owl:Restriction ;
            owl:allValuesFrom UO:0000000 ;
            owl:onProperty fsi:has_unit ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:double ;
            owl:onProperty fsi:has_numeric_value ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:has_unit ],
        fsi:Entity ;
    skos:definition "A value of an attribute that is quantitative and measurable, expressed as a combination of a unit and a numeric value" ;
    skos:inScheme fsi: .

fsi:has_numeric_value a owl:DatatypeProperty ;
    rdfs:label "has numeric value" ;
    rdfs:domain fsi:QuantityValue ;
    rdfs:range xsd:double ;
    skos:definition "connects a quantity value to a number" ;
    skos:exactMatch qudt:quantityValue ;
    skos:inScheme fsi: .

fsi:has_unit a owl:DatatypeProperty ;
    rdfs:label "has unit" ;
    rdfs:domain fsi:QuantityValue ;
    rdfs:range UO:0000000 ;
    skos:definition "connects a quantity value to a unit" ;
    skos:exactMatch IAO:0000039,
        qudt:unit ;
    skos:inScheme fsi: .

fsi:iri a owl:DatatypeProperty ;
    rdfs:label "iri" ;
    rdfs:range xsd:string ;
    skos:definition "An IRI for an entity. This is determined by the id using expansion rules." ;
    skos:inScheme fsi: .

fsi:measurement_of a owl:ObjectProperty ;
    rdfs:label "measurement_of" ;
    dct:title "measurement of" ;
    rdfs:range fsi:Indicator ;
    rdfs:subPropertyOf fsi:node_property ;
    skos:definition "Indicator" ;
    skos:inScheme fsi: .

fsi:spatial_scope a owl:ObjectProperty ;
    rdfs:label "spatial_scope" ;
    dct:title "spatial scope" ;
    rdfs:range fsi:SpatialScopeType ;
    skos:definition "Reference to the spatial unit that the indicator describes" ;
    skos:inScheme fsi: .

fsi:EconomicThematicArea a owl:Class ;
    owl:unionOf ( <https://w3id.org/linkml/examples/fsiEconomicThematicArea#FairEconomicViabilityInFoodValueChain> <https://w3id.org/linkml/examples/fsiEconomicThematicArea#DevelopmentAndLogistics> ) ;
    linkml:permissible_values <https://w3id.org/linkml/examples/fsiEconomicThematicArea#DevelopmentAndLogistics>,
        <https://w3id.org/linkml/examples/fsiEconomicThematicArea#FairEconomicViabilityInFoodValueChain> .

fsi:Entity a owl:Class ;
    rdfs:label "Entity" ;
    dct:title "entity" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty fsi:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:description ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty fsi:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:iri ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty fsi:name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty fsi:iri ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:iri ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:id ] ;
    skos:definition "Root class for all things and informational relationships, real or imagined." ;
    skos:inScheme fsi: .

fsi:HorizontalThematicArea a owl:Class ;
    owl:unionOf ( <https://w3id.org/linkml/examples/fsiHorizontalThematicArea#Governance> <https://w3id.org/linkml/examples/fsiHorizontalThematicArea#Resilience> ) ;
    linkml:permissible_values <https://w3id.org/linkml/examples/fsiHorizontalThematicArea#Governance>,
        <https://w3id.org/linkml/examples/fsiHorizontalThematicArea#Resilience> .

fsi:Indicator a owl:Class ;
    rdfs:label "Indicator" ;
    dct:title "indicator" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:key_area ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty fsi:thematic_area ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom fsi:SustainabilityDimension ;
            owl:onProperty fsi:key_area ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty fsi:name ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty fsi:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:spatial_scope ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ owl:intersectionOf ( [ owl:unionOf ( fsi:EnvironmentalThematicArea fsi:EconomicThematicArea fsi:SocialThematicArea fsi:HorizontalThematicArea ) ] fsi:ThematicArea ) ] ;
            owl:onProperty fsi:thematic_area ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty fsi:spatial_scope ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:thematic_area ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:id ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty fsi:key_area ],
        [ a owl:Restriction ;
            owl:allValuesFrom fsi:SpatialScopeType ;
            owl:onProperty fsi:spatial_scope ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty fsi:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:id ],
        fsi:Entity ;
    skos:definition "Food system indicator." ;
    skos:inScheme fsi: .

fsi:SocialThematicArea a owl:Class ;
    owl:unionOf ( <https://w3id.org/linkml/examples/fsiSocialThematicArea#FairInclusiveAndEthicalFoodSystem> <https://w3id.org/linkml/examples/fsiSocialThematicArea#FoodEnvironment> <https://w3id.org/linkml/examples/fsiSocialThematicArea#NutritionAndHealth> ) ;
    linkml:permissible_values <https://w3id.org/linkml/examples/fsiSocialThematicArea#FairInclusiveAndEthicalFoodSystem>,
        <https://w3id.org/linkml/examples/fsiSocialThematicArea#FoodEnvironment>,
        <https://w3id.org/linkml/examples/fsiSocialThematicArea#NutritionAndHealth> .

fsi:SpatialScopeType a owl:Class ;
    owl:unionOf ( <https://w3id.org/linkml/examples/fsiSpatialScopeType#Eu> <https://w3id.org/linkml/examples/fsiSpatialScopeType#EuMemberStates> <https://w3id.org/linkml/examples/fsiSpatialScopeType#Regional> <https://w3id.org/linkml/examples/fsiSpatialScopeType#Local> ) ;
    linkml:permissible_values <https://w3id.org/linkml/examples/fsiSpatialScopeType#Eu>,
        <https://w3id.org/linkml/examples/fsiSpatialScopeType#EuMemberStates>,
        <https://w3id.org/linkml/examples/fsiSpatialScopeType#Local>,
        <https://w3id.org/linkml/examples/fsiSpatialScopeType#Regional> .

fsi:SustainabilityDimension a owl:Class ;
    owl:unionOf ( <https://w3id.org/linkml/examples/fsiSustainabilityDimension#Environmental> <https://w3id.org/linkml/examples/fsiSustainabilityDimension#Economic> <https://w3id.org/linkml/examples/fsiSustainabilityDimension#Social> <https://w3id.org/linkml/examples/fsiSustainabilityDimension#Horizontal> ) ;
    linkml:permissible_values <https://w3id.org/linkml/examples/fsiSustainabilityDimension#Economic>,
        <https://w3id.org/linkml/examples/fsiSustainabilityDimension#Environmental>,
        <https://w3id.org/linkml/examples/fsiSustainabilityDimension#Horizontal>,
        <https://w3id.org/linkml/examples/fsiSustainabilityDimension#Social> .

fsi:description a owl:DatatypeProperty ;
    rdfs:label "description" ;
    rdfs:range xsd:string ;
    skos:altLabel "definition" ;
    skos:definition "A human-readable description of an entity." ;
    skos:exactMatch IAO:0000115,
        skos:definitions ;
    skos:inScheme fsi: .

fsi:name a owl:DatatypeProperty ;
    rdfs:label "name" ;
    dct:title "name" ;
    rdfs:domain fsi:Entity ;
    rdfs:range xsd:string ;
    skos:altLabel "display name",
        "label",
        "title" ;
    skos:definition "A human-readable name for an attribute or entity." ;
    skos:inScheme fsi: ;
    skos:narrowMatch dct:title .

fsi:key_area a owl:ObjectProperty ;
    rdfs:label "key_area" ;
    dct:title "key area" ;
    rdfs:range fsi:SustainabilityDimension ;
    skos:definition "Reference to the thematic area that the indicator belongs to" ;
    skos:inScheme fsi: .

fsi:thematic_area a owl:ObjectProperty ;
    rdfs:label "thematic_area" ;
    dct:title "thematic area" ;
    rdfs:range [ owl:intersectionOf ( [ owl:unionOf ( fsi:EnvironmentalThematicArea fsi:EconomicThematicArea fsi:SocialThematicArea fsi:HorizontalThematicArea ) ] fsi:ThematicArea ) ] ;
    skos:definition "the thematic area the indicator belongs to" ;
    skos:inScheme fsi: .

fsi:EnvironmentalThematicArea a owl:Class ;
    owl:unionOf ( <https://w3id.org/linkml/examples/fsiEnvironmentalThematicArea#ClimateChange> <https://w3id.org/linkml/examples/fsiEnvironmentalThematicArea#PollutionAntimicrobials> <https://w3id.org/linkml/examples/fsiEnvironmentalThematicArea#SustainableUseOfResources> <https://w3id.org/linkml/examples/fsiEnvironmentalThematicArea#Biodiversity> <https://w3id.org/linkml/examples/fsiEnvironmentalThematicArea#CrossCuttingAreas> ) ;
    linkml:permissible_values <https://w3id.org/linkml/examples/fsiEnvironmentalThematicArea#Biodiversity>,
        <https://w3id.org/linkml/examples/fsiEnvironmentalThematicArea#ClimateChange>,
        <https://w3id.org/linkml/examples/fsiEnvironmentalThematicArea#CrossCuttingAreas>,
        <https://w3id.org/linkml/examples/fsiEnvironmentalThematicArea#PollutionAntimicrobials>,
        <https://w3id.org/linkml/examples/fsiEnvironmentalThematicArea#SustainableUseOfResources> .

fsi:id a owl:DatatypeProperty ;
    rdfs:label "id" ;
    rdfs:domain fsi:Entity ;
    skos:definition "A unique identifier for an entity." ;
    skos:inScheme fsi: .

xsd:string a rdfs:Datatype ;
    owl:equivalentClass xsd:anyURI .

[] rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty fsi:thematic_area ;
            owl:someValuesFrom fsi:EnvironmentalThematicArea ] ;
    owl:intersectionOf ( [ a owl:Restriction ;
                owl:onProperty fsi:key_area ;
                owl:someValuesFrom xsd:string ] fsi:Indicator ) .

[] rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty fsi:thematic_area ;
            owl:someValuesFrom fsi:HorizontalThematicArea ] ;
    owl:intersectionOf ( [ a owl:Restriction ;
                owl:onProperty fsi:key_area ;
                owl:someValuesFrom xsd:string ] fsi:Indicator ) .

[] rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty fsi:thematic_area ;
            owl:someValuesFrom fsi:EconomicThematicArea ] ;
    owl:intersectionOf ( [ a owl:Restriction ;
                owl:onProperty fsi:key_area ;
                owl:someValuesFrom xsd:string ] fsi:Indicator ) .

[] rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty fsi:thematic_area ;
            owl:someValuesFrom fsi:SocialThematicArea ] ;
    owl:intersectionOf ( [ a owl:Restriction ;
                owl:onProperty fsi:key_area ;
                owl:someValuesFrom xsd:string ] fsi:Indicator ) .


