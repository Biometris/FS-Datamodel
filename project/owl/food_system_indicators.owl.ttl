@prefix IAO: <http://purl.obolibrary.org/obo/IAO_> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix fsi: <https://w3id.org/linkml/examples/fsi> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

fsi:.owl.ttl a owl:Ontology ;
    rdfs:label "food-system-indicators" .

fsi:Datapoint a owl:Class ;
    rdfs:label "datapoint" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:datapoint_of ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:datapoint_of ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty fsi:datapoint_of ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty fsi:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:id ],
        fsi:QuantityValue ;
    skos:definition "Food system indicator datapoint." ;
    skos:inScheme fsi: .

fsi:synonym a owl:DatatypeProperty ;
    rdfs:label "synonym" ;
    rdfs:domain fsi:NamedThing ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf fsi:node_property ;
    skos:altLabel "alias" ;
    skos:definition "Alternate human-readable names for a thing" ;
    skos:inScheme fsi: .

fsi:Indicator a owl:Class ;
    rdfs:label "indicator" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty fsi:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:name ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty fsi:name ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty fsi:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:spatial_scope ],
        [ a owl:Restriction ;
            owl:allValuesFrom fsi:SpatialScopeTypes ;
            owl:onProperty fsi:spatial_scope ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty fsi:spatial_scope ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:id ],
        fsi:NamedThing ;
    skos:definition "Food system indicator." ;
    skos:inScheme fsi: .

<UO:0000000> a rdfs:Datatype ;
    owl:equivalentClass xsd:string .

<https://w3id.org/linkml/examples/fsiSpatialScopeTypes#Eu> a owl:Class ;
    rdfs:label "Eu" ;
    rdfs:subClassOf fsi:SpatialScopeTypes .

<https://w3id.org/linkml/examples/fsiSpatialScopeTypes#EuMemberStates> a owl:Class ;
    rdfs:label "EuMemberStates" ;
    rdfs:subClassOf fsi:SpatialScopeTypes .

<https://w3id.org/linkml/examples/fsiSpatialScopeTypes#Local> a owl:Class ;
    rdfs:label "Local" ;
    rdfs:subClassOf fsi:SpatialScopeTypes .

<https://w3id.org/linkml/examples/fsiSpatialScopeTypes#Regional> a owl:Class ;
    rdfs:label "Regional" ;
    rdfs:subClassOf fsi:SpatialScopeTypes .

fsi:node_property a owl:DatatypeProperty ;
    rdfs:label "node property" ;
    rdfs:domain fsi:NamedThing ;
    skos:definition "A grouping for any property that holds between a node and a value." ;
    skos:inScheme fsi: .

fsi:Entity a owl:Class ;
    rdfs:label "entity" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty fsi:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:iri ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty fsi:name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:iri ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:description ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty fsi:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty fsi:iri ] ;
    skos:definition "Root class for all things and informational relationships, real or imagined." ;
    skos:inScheme fsi: .

fsi:QuantityValue a owl:Class ;
    rdfs:label "quantity value" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty fsi:has_numeric_value ],
        [ a owl:Restriction ;
            owl:allValuesFrom <UO:0000000> ;
            owl:onProperty fsi:has_unit ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:double ;
            owl:onProperty fsi:has_numeric_value ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty fsi:has_unit ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:has_numeric_value ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:has_unit ],
        fsi:NamedThing ;
    skos:definition "A value of an attribute that is quantitative and measurable, expressed as a combination of a unit and a numeric value" ;
    skos:inScheme fsi: .

fsi:datapoint_of a owl:DatatypeProperty,
        owl:ObjectProperty ;
    rdfs:label "datapoint of" ;
    rdfs:range fsi:Indicator ;
    rdfs:subPropertyOf fsi:node_property ;
    skos:definition "Indicator" ;
    skos:inScheme fsi: .

fsi:has_numeric_value a owl:DatatypeProperty ;
    rdfs:label "has numeric value" ;
    rdfs:domain fsi:QuantityValue ;
    rdfs:range xsd:double ;
    skos:definition "connects a quantity value to a number" ;
    skos:exactMatch qudt:quantityValue ;
    skos:inScheme fsi: .

fsi:has_unit a owl:DatatypeProperty ;
    rdfs:label "has unit" ;
    rdfs:domain fsi:QuantityValue ;
    rdfs:range <UO:0000000> ;
    skos:definition "connects a quantity value to a unit" ;
    skos:exactMatch IAO:0000039,
        qudt:unit ;
    skos:inScheme fsi: .

fsi:iri a owl:DatatypeProperty ;
    rdfs:label "iri" ;
    rdfs:range xsd:string ;
    skos:definition "An IRI for an entity. This is determined by the id using expansion rules." ;
    skos:inScheme fsi: .

fsi:spatial_scope a owl:ObjectProperty ;
    rdfs:label "spatial scope" ;
    skos:definition "reference to the spatial unit that the indicator describes" ;
    skos:inScheme fsi: .

fsi:NamedThing a owl:Class ;
    rdfs:label "named thing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty fsi:category ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty fsi:category ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty fsi:category ],
        fsi:Entity ;
    skos:definition "A databased entity or concept/class." ;
    skos:inScheme fsi: .

fsi:SpatialScopeTypes a owl:Class ;
    owl:unionOf ( <https://w3id.org/linkml/examples/fsiSpatialScopeTypes#Eu> <https://w3id.org/linkml/examples/fsiSpatialScopeTypes#EuMemberStates> <https://w3id.org/linkml/examples/fsiSpatialScopeTypes#Regional> <https://w3id.org/linkml/examples/fsiSpatialScopeTypes#Local> ) ;
    linkml:permissible_values <https://w3id.org/linkml/examples/fsiSpatialScopeTypes#Eu>,
        <https://w3id.org/linkml/examples/fsiSpatialScopeTypes#EuMemberStates>,
        <https://w3id.org/linkml/examples/fsiSpatialScopeTypes#Local>,
        <https://w3id.org/linkml/examples/fsiSpatialScopeTypes#Regional> .

fsi:description a owl:DatatypeProperty ;
    rdfs:label "description" ;
    rdfs:range xsd:string ;
    skos:altLabel "definition" ;
    skos:definition "A human-readable description of an entity.",
        "concise text that provides the meaning of the identifier" ;
    skos:exactMatch IAO:0000115,
        skos:definitions ;
    skos:inScheme fsi: .

fsi:name a owl:DatatypeProperty ;
    rdfs:label "name" ;
    rdfs:domain fsi:Entity ;
    rdfs:range xsd:string ;
    skos:altLabel "display name",
        "label",
        "title" ;
    skos:definition "A human-readable name for an attribute or entity.",
        "denomination of the indicator" ;
    skos:inScheme fsi: ;
    skos:narrowMatch dct:title .

fsi:id a owl:DatatypeProperty ;
    rdfs:label "id" ;
    rdfs:domain fsi:Entity ;
    skos:definition "A unique identifier for an entity.",
        "unique identifier of the indicator" ;
    skos:inScheme fsi: .

xsd:string a rdfs:Datatype ;
    owl:equivalentClass [ owl:intersectionOf ( xsd:anyURI <xsd:string> ) ],
        <xsd:string> .


