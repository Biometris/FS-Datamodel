    {
      "name": "SustainabilityDimension",
      "definition_uri": "https://w3id.org/linkml/examples/fsiSustainabilityDimension",
      "description": "A core pillar of sustainability (environment, society, economy, or health) that groups indicators to evaluate different aspects of sustainable development within the food system.",
      "title": "sustainability dimension",
      "from_schema": "https://w3id.org/linkml/examples/fsi",
      "permissible_values": [
        {
          "text": "Environment",
          "description": "Sustainability dimension that focuses on the state and management of natural resources and ecosystems, including aspects such as climate, biodiversity, pollution, and resource efficiency, to ensure the food system operates within ecological limits.",
          "title": "environment"
        },
        {
          "text": "Economy",
          "description": "Sustainability dimension that focuses on the performance, efficiency, and resilience of economic activities within the food system, including aspects like productivity, value distribution, competitiveness, and financial viability.",
          "title": "economy"
        },
        {
          "text": "Society",
          "description": "Sustainability dimension that focuses on human well-being, equity, and societal outcomes within the food system, including aspects such as health, nutrition, fairness, labor conditions, and access to resources.",
          "title": "society"
        },
        {
          "text": "Health",
          "description": "Sustainability dimension that focucus on ... .",
          "title": "health"
        }
      ]
