{
    "$defs": {
        "CriterionCategory": {
            "description": "Category of an indicator selection criterion.",
            "enum": [
                "GoalOrientedAndPolicyRelevant",
                "SoundMethodologies",
                "AvailabilityAndAccessibility",
                "GeographicalCoverageAndTimeliness",
                "OperationalFeasibility"
            ],
            "title": "CriterionCategory",
            "type": "string"
        },
        "Database": {
            "additionalProperties": false,
            "description": "Database containing data corresponding to an indicator.",
            "properties": {
                "database_link": {
                    "description": "Link to the database",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "description": {
                    "description": "A human-readable description of an entity.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "id": {
                    "description": "A unique identifier for an entity.",
                    "type": "string"
                },
                "iri": {
                    "description": "An IRI for an entity. This is determined by the id using expansion rules.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "name": {
                    "description": "A human-readable name for an attribute or entity.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "update_frequency": {
                    "$ref": "#/$defs/TimeGranularity",
                    "description": "Frequency at which the data is updated."
                }
            },
            "required": [
                "id"
            ],
            "title": "Database",
            "type": "object"
        },
        "Domain": {
            "additionalProperties": true,
            "description": "A specific sub-area within a thematic area that groups related indicators to measure a particular aspect of sustainability.",
            "title": "Domain",
            "type": [
                "null",
                "boolean",
                "object",
                "number",
                "string"
            ]
        },
        "EconomicDomain": {
            "description": "A specific area within the economic dimension that focuses on the performance, resilience, and fairness of economic activities. Examples are productivity, value distribution, or competitiveness, measured through related indicators.",
            "enum": [
                "SectorialGrowth",
                "MarketPowerAndBusinessStructure",
                "IncomeDistribution",
                "Price",
                "Trade",
                "TechnologyAndDigitalization",
                "TransportAccessibilityAndInfrastructure"
            ],
            "title": "EconomicDomain",
            "type": "string"
        },
        "EconomicThematicArea": {
            "description": "Thematic areas defined for the economic sustainability dimension.",
            "enum": [
                "FairEconomicViabilityInFoodValueChain",
                "DevelopmentAndLogistics"
            ],
            "title": "EconomicThematicArea",
            "type": "string"
        },
        "EnvironmentalDomain": {
            "description": "A specific area within the environmental dimension that focuses on the state and use of natural resources and ecosystems. Examples are climate impact, biodiversity, pollution, or resource efficiency, measured through related indicators.",
            "enum": [
                "GHGEmissions",
                "Pollution",
                "Antimicrobials",
                "LandAndSoil",
                "Water",
                "AquaticLivingResources",
                "Energy",
                "BiodiversityConservationAndRestorationOfNaturalEcosystems",
                "FoodLossAndWaste",
                "ConsumptionFootprint"
            ],
            "title": "EnvironmentalDomain",
            "type": "string"
        },
        "EnvironmentalThematicArea": {
            "description": "Thematic areas defined for the enviromental sustainability dimension.",
            "enum": [
                "ClimateChange",
                "PollutionAntimicrobials",
                "SustainableUseOfResources",
                "Biodiversity",
                "CrossCuttingAreas"
            ],
            "title": "EnvironmentalThematicArea",
            "type": "string"
        },
        "HorizontalDomain": {
            "description": "A cross-cutting area that spans all sustainability dimensions\u2014environmental, social, and economic\u2014and addresses overarching aspects such as governance, resilience, or innovation that influence the entire food system.",
            "enum": [
                "StrategicPlanningAndPolicies",
                "EffectiveImplementation",
                "Accountability",
                "SharedVision",
                "ShockResilience",
                "Preparedness",
                "Adaptation",
                "Transformation"
            ],
            "title": "HorizontalDomain",
            "type": "string"
        },
        "HorizontalThematicArea": {
            "description": "Thematic areas defined for the horizontal, cross-cutting sustainability dimension.",
            "enum": [
                "Governance",
                "Resilience"
            ],
            "title": "HorizontalThematicArea",
            "type": "string"
        },
        "Indicator": {
            "additionalProperties": false,
            "allOf": [
                {
                    "if": {
                        "properties": {
                            "key_area": {
                                "const": "environmental"
                            }
                        },
                        "required": [
                            "key_area"
                        ]
                    },
                    "then": {
                        "properties": {
                            "thematic_area": {
                                "anyOf": [
                                    {
                                        "$ref": "#/$defs/EnvironmentalThematicArea"
                                    }
                                ]
                            }
                        },
                        "required": [
                            "thematic_area"
                        ]
                    }
                },
                {
                    "if": {
                        "properties": {
                            "key_area": {
                                "const": "economic"
                            }
                        },
                        "required": [
                            "key_area"
                        ]
                    },
                    "then": {
                        "properties": {
                            "thematic_area": {
                                "anyOf": [
                                    {
                                        "$ref": "#/$defs/EconomicThematicArea"
                                    }
                                ]
                            }
                        },
                        "required": [
                            "thematic_area"
                        ]
                    }
                },
                {
                    "if": {
                        "properties": {
                            "key_area": {
                                "const": "social"
                            }
                        },
                        "required": [
                            "key_area"
                        ]
                    },
                    "then": {
                        "properties": {
                            "thematic_area": {
                                "anyOf": [
                                    {
                                        "$ref": "#/$defs/SocialThematicArea"
                                    }
                                ]
                            }
                        },
                        "required": [
                            "thematic_area"
                        ]
                    }
                },
                {
                    "if": {
                        "properties": {
                            "key_area": {
                                "const": "horizontal"
                            }
                        },
                        "required": [
                            "key_area"
                        ]
                    },
                    "then": {
                        "properties": {
                            "thematic_area": {
                                "anyOf": [
                                    {
                                        "$ref": "#/$defs/HorizontalThematicArea"
                                    }
                                ]
                            }
                        },
                        "required": [
                            "thematic_area"
                        ]
                    }
                }
            ],
            "description": "A quantitative measure used to assess the state, performance, or progress of a specific aspect of the food system in relation to sustainability objectives.",
            "properties": {
                "description": {
                    "description": "Concise text that provides the meaning of the identifier.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "granularity": {
                    "description": "Reference to the granularity(ies) for which the indicator is measured.",
                    "items": {
                        "$ref": "#/$defs/SpatialGranularity"
                    },
                    "type": "array"
                },
                "id": {
                    "description": "Unique identifier of the indicator.",
                    "type": "string"
                },
                "indicator_domain": {
                    "$ref": "#/$defs/Domain",
                    "anyOf": [
                        {
                            "$ref": "#/$defs/EnvironmentalDomain"
                        },
                        {
                            "$ref": "#/$defs/EconomicDomain"
                        },
                        {
                            "$ref": "#/$defs/SocialDomain"
                        },
                        {
                            "$ref": "#/$defs/HorizontalDomain"
                        }
                    ],
                    "description": "Reference to the domain that the indicator belongs to."
                },
                "iri": {
                    "description": "An IRI for an entity. This is determined by the id using expansion rules.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "key_area": {
                    "$ref": "#/$defs/SustainabilityDimension",
                    "description": "Reference to the thematic area that the indicator belongs to."
                },
                "measurement_unit": {
                    "description": "Unit of measurement.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "name": {
                    "description": "Denomination of the indicator.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "supply_chain_component": {
                    "description": "Reference to the supply chain component(s) for which the indicator is relevant.",
                    "items": {
                        "$ref": "#/$defs/SupplyChainComponentType"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                },
                "thematic_area": {
                    "$ref": "#/$defs/ThematicArea",
                    "anyOf": [
                        {
                            "$ref": "#/$defs/EnvironmentalThematicArea"
                        },
                        {
                            "$ref": "#/$defs/EconomicThematicArea"
                        },
                        {
                            "$ref": "#/$defs/SocialThematicArea"
                        },
                        {
                            "$ref": "#/$defs/HorizontalThematicArea"
                        }
                    ],
                    "description": "The thematic area the indicator belongs to."
                }
            },
            "required": [
                "key_area",
                "thematic_area",
                "indicator_domain",
                "granularity",
                "id"
            ],
            "title": "Indicator",
            "type": "object"
        },
        "IndicatorCriterion": {
            "additionalProperties": false,
            "description": "Criterion used for indicator selection.",
            "properties": {
                "category": {
                    "$ref": "#/$defs/CriterionCategory",
                    "description": "category for the indicator selection criterion."
                },
                "description": {
                    "description": "A human-readable description of an entity.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "id": {
                    "description": "A unique identifier for an entity.",
                    "type": "string"
                },
                "iri": {
                    "description": "An IRI for an entity. This is determined by the id using expansion rules.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "name": {
                    "description": "A human-readable name for an attribute or entity.",
                    "type": [
                        "string",
                        "null"
                    ]
                }
            },
            "required": [
                "id"
            ],
            "title": "IndicatorCriterion",
            "type": "object"
        },
        "IndicatorDataCollectionDetails": {
            "additionalProperties": false,
            "description": "Details on the collection of indicator data within a certain database/dataset.",
            "properties": {
                "data_link": {
                    "description": "Link to the data for the indicator",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "description": {
                    "description": "A human-readable description of an entity.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "id": {
                    "description": "A unique identifier for an entity.",
                    "type": "string"
                },
                "in_database": {
                    "description": "Specifies the database which the entity is part of.",
                    "type": "string"
                },
                "iri": {
                    "description": "An IRI for an entity. This is determined by the id using expansion rules.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "measures_indicator": {
                    "description": "Indicator.",
                    "type": "string"
                },
                "name": {
                    "description": "A human-readable name for an attribute or entity.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "newest_datapoint": {
                    "description": "Newest datapoint for which the indicator is measured.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "oldest_datapoint": {
                    "description": "Oldest datapoint for which the indicator is measured.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "spatial_granularity": {
                    "$ref": "#/$defs/SpatialGranularity",
                    "description": "Spatial granularity/resolution of the datapoints."
                },
                "spatial_scope": {
                    "$ref": "#/$defs/SpatialScopeType",
                    "description": "Spatial scope of the data collection."
                },
                "time_granularity": {
                    "$ref": "#/$defs/TimeGranularity",
                    "description": "Time granularity between the datapoints."
                }
            },
            "required": [
                "measures_indicator",
                "in_database",
                "id"
            ],
            "title": "IndicatorDataCollectionDetails",
            "type": "object"
        },
        "IndicatorDatapoint": {
            "additionalProperties": false,
            "description": "A food system indicator datapoint is a measured value or statistic that provides quantitative information for a specific sustainability indicator at a given time and place, used to track the performance or state of the food system.",
            "properties": {
                "description": {
                    "description": "A human-readable description of an entity.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "has_numeric_value": {
                    "description": "connects a quantity value to a number",
                    "type": [
                        "number",
                        "null"
                    ]
                },
                "has_unit": {
                    "description": "connects a quantity value to a unit",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "id": {
                    "description": "Unique identifier of the datapoint.",
                    "type": "string"
                },
                "iri": {
                    "description": "An IRI for an entity. This is determined by the id using expansion rules.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "measurement_of": {
                    "description": "Indicator.",
                    "type": "string"
                },
                "name": {
                    "description": "A human-readable name for an attribute or entity.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "part_of_collection": {
                    "description": "Data collection which this measurement is part of.",
                    "type": "string"
                }
            },
            "required": [
                "measurement_of",
                "part_of_collection",
                "id"
            ],
            "title": "IndicatorDatapoint",
            "type": "object"
        },
        "IndicatorcriteriaScore": {
            "additionalProperties": false,
            "description": "Score for an indicator for an indicator selection criterion.",
            "properties": {
                "comment": {
                    "description": "A comment about an entity.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "score": {
                    "$ref": "#/$defs/ScoreCategory",
                    "description": "score for an indicator selection criterion for an indicator."
                },
                "score_for_indicator": {
                    "description": "Indicator.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "scores_criterion": {
                    "description": "Indicator criterion.",
                    "type": [
                        "string",
                        "null"
                    ]
                }
            },
            "title": "IndicatorcriteriaScore",
            "type": "object"
        },
        "QuantityValue": {
            "additionalProperties": false,
            "description": "A value of an attribute that is quantitative and measurable, expressed as a combination of a unit and a numeric value",
            "properties": {
                "description": {
                    "description": "A human-readable description of an entity.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "has_numeric_value": {
                    "description": "connects a quantity value to a number",
                    "type": [
                        "number",
                        "null"
                    ]
                },
                "has_unit": {
                    "description": "connects a quantity value to a unit",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "id": {
                    "description": "A unique identifier for an entity.",
                    "type": "string"
                },
                "iri": {
                    "description": "An IRI for an entity. This is determined by the id using expansion rules.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "name": {
                    "description": "A human-readable name for an attribute or entity.",
                    "type": [
                        "string",
                        "null"
                    ]
                }
            },
            "required": [
                "id"
            ],
            "title": "QuantityValue",
            "type": "object"
        },
        "ScoreCategory": {
            "description": "Score category.",
            "enum": [
                "VeryPoor",
                "Poor",
                "Moderate",
                "Good",
                "Excellent"
            ],
            "title": "ScoreCategory",
            "type": "string"
        },
        "SocialDomain": {
            "description": "Specific area within the social dimension of sustainability that focuses on human well-being, equity, and social conditions. Examples are fairness, health, nutrition, or working conditions, measured through related indicators.",
            "enum": [
                "Employment",
                "AnimalWelfare",
                "FoodHeritage",
                "FoodAffordability",
                "FoodAvailability",
                "HealthImpactFromDiets",
                "FoodSecurity"
            ],
            "title": "SocialDomain",
            "type": "string"
        },
        "SocialThematicArea": {
            "description": "Thematic areas defined for the social sustainability dimension.",
            "enum": [
                "FairInclusiveAndEthicalFoodSystem",
                "FoodEnvironment",
                "NutritionAndHealth"
            ],
            "title": "SocialThematicArea",
            "type": "string"
        },
        "SpatialGranularity": {
            "description": "The granularity for which the indicator is measured.",
            "enum": [
                "MacroRegions",
                "Country",
                "NUTS1",
                "NUTS2",
                "NUTS3",
                "LAU"
            ],
            "title": "SpatialGranularity",
            "type": "string"
        },
        "SpatialScopeType": {
            "description": "The spatial scope for which the indicator is defined.",
            "enum": [
                "Global",
                "Eu",
                "EuMemberStates",
                "Regional",
                "Local"
            ],
            "title": "SpatialScopeType",
            "type": "string"
        },
        "SupplyChainComponentType": {
            "description": "The supply chain component for which the indicator is relevant.",
            "enum": [
                "PrimaryFoodProduction",
                "FoodProcessing",
                "FoodDistribution",
                "FoodConsumption"
            ],
            "title": "SupplyChainComponentType",
            "type": "string"
        },
        "SustainabilityDimension": {
            "description": "A core pillar of sustainability (environmental, social, economic, or horizontal) that groups indicators to evaluate different aspects of sustainable development within the food system.",
            "enum": [
                "Environmental",
                "Economic",
                "Social",
                "Horizontal"
            ],
            "title": "SustainabilityDimension",
            "type": "string"
        },
        "ThematicArea": {
            "additionalProperties": true,
            "description": "A topic within a sustainability dimension (environmental, social, or economic) that organizes related domains and indicators around a common sustainability theme.",
            "title": "ThematicArea",
            "type": [
                "null",
                "boolean",
                "object",
                "number",
                "string"
            ]
        },
        "TimeGranularity": {
            "description": "Time granularity for measuring and updating data.",
            "enum": [
                "EveryTwoYears",
                "Yearly",
                "Monthly",
                "Weekly",
                "Other"
            ],
            "title": "TimeGranularity",
            "type": "string"
        }
    },
    "$id": "https://w3id.org/linkml/examples/fsi",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "additionalProperties": true,
    "metamodel_version": "1.7.0",
    "title": "food_system_indicators",
    "type": "object",
    "version": null
}
